{
    "name": "winwin/job-queue",
    "type": "project",
    "require": {
        "php": ">= 5.6",
        "ext-pcntl": "*",
        "ext-posix": "*",
        "ext-json": "*",
        "pda/pheanstalk": "^3.1",
        "symfony/event-dispatcher": "^3.2",
        "psr/container": "^1.0",
        "mtdowling/cron-expression": "^1.1",
        "symfony/process": "^3.2",
        "symfony/filesystem": "^3.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0",
        "winwin/composer-phpcs-plugin": "^0.1",
        "vlucas/phpdotenv": "^2.4",
        "nesbot/carbon": "^1.22",
        "guzzlehttp/guzzle": "^6.2",
        "monolog/monolog": "^1.22"
    },
    "autoload": {
        "psr-4": {
            "winwin\\jobQueue\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "winwin\\jobQueue\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "0.2-dev"
        }
    }
}
